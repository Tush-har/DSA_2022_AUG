#include <iostream>

using namespace std;

int main()
{
   
        int arr[50], size, i;
        bool c;
    

        cout<<"Enter array size( Max:50 ) :: ";
        cin>>size;
        cout<<"\nEnter array elements :: \n";

        for(i=0; i<size; i++)
        {
            cout<<"\nEnter arr["<<i<<"] Element :: ";
                cin>>arr[i];
        }
        for(i=1;i<size;i++){
            if(arr[i]<arr[i-1])
            { cout<<"false"<<endl;
              }
        cout<<"true"<<endl;
        }
        // cout<<c;
      

    return 0;
}
