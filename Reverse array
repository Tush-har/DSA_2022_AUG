

#include <iostream>

using namespace std;

int main()
{
   
        int arr[50], size, i,temp,low,high;


        cout<<"Enter array size( Max:50 ) :: ";
        cin>>size;
        cout<<"\nEnter array elements :: \n";

        for(i=0; i<size; i++)
        {
            cout<<"\nEnter arr["<<i<<"] Element :: ";
                cin>>arr[i];
        }
        low=0;
        high=size-1;
        while(low<high){
            temp=arr[low];
            arr[low]=arr[high];
            arr[high]=temp;
            low++;
            high--;
        }
           for(i=0; i<size; i++)
        {
            
                cout<<arr[i]<<endl;
        }
      

    return 0;
}
